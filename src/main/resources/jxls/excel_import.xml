<?xml version="1.0" encoding="UTF-8"?>
<workbook>
<worksheet idx="0">
    <section startRow="0" endRow="0"/>
    <loop startRow="1" endRow="1" items="excelBeans" var="main"
          varType="com.harry.bean.ExcelBean">
        <section startRow="1" endRow="1">
            <mapping cell="A2">main.hospitalId</mapping>
            <mapping cell="B2">main.hospitalName</mapping>
        </section>
        <loopbreakcondition>
            <rowcheck offset="0">
                <cellcheck offset="0"/>
            </rowcheck>
        </loopbreakcondition>
    </loop>
</worksheet>
</workbook>

